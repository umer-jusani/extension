{
  "manifest_version": 3,
  "name": "React Chrome Extension",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/feed/"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["storage"]
}
